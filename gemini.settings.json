{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ]
    },
    "azure": {
      "command": "npx",
      "args": [
        "-y",
        "@microsoft/azure-mcp-server"
      ]
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tool": {
    "confirmationMode": "auto"
  }
}
